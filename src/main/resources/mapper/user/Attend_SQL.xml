<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 

<mapper namespace="attend"> 

	<resultMap id="attendList" type="first.vassystem.dto.AttendList">
 		<result property = "dayNo" column = "day_no"/>
 		<result property = "rwdType" column = "rwd_type"/>
 		<result property = "rwdId" column = "rwd_id"/>
 		<result property = "rwdRcvFlag" column = "rwd_rcv_flag"/>
 		<result property = "rwdName" column = "rwd_nm"/>
 		<result property = "todayNo" column = "today_no"/>
	</resultMap>

	<select id="selectAttendList" statementType="CALLABLE" parameterType="int" resultMap ="attendList"> 
	{ 
		call abn_getUserAttendList (
			1,
			1,
			#{user_account, jdbcType = INTEGER, mode = IN},
			null
		)
	}
	</select>

	<select id="mgmtAttend" statementType="CALLABLE" parameterType="first.common.util.ParamVO"> 
	{ 
		call abn_MgmtAttend (
								#{inParam01,mode=IN,jdbcType=INTEGER},
								#{inParam02,mode=IN,jdbcType=INTEGER},
								#{inParam03,mode=IN,jdbcType=INTEGER},
								#{inParam04,mode=IN,jdbcType=INTEGER},
								#{outParam01,mode=OUT,jdbcType=INTEGER})
	}
	</select>
</mapper>