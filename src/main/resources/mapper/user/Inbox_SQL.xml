<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 

<mapper namespace="inbox"> 

	<resultMap id="inboxList" type="first.vassystem.dto.InboxList">
 		<result property = "rcvNo" column = "receive_sn"/>
 		<result property = "senderAccount" column = "sender_account"/>
 		<result property = "rwdType" column = "good_type"/>
 		<result property = "rwdId" column = "good_id"/>
 		<result property = "rwdName" column = "good_nm"/>
 		<result property = "remainDay" column = "remain_day"/>
	</resultMap>

	<select id="selectInboxList" statementType="CALLABLE" parameterType="int" resultMap ="inboxList"> 
	{ 
		call abn_getMsgList (
			1
			,#{user_account, jdbcType = INTEGER, mode = IN}
		)
	}
	</select>

	<select id="mgmtInbox" statementType="CALLABLE" parameterType="first.common.util.ParamVO"> 
	{ 
		call abn_mgmtReceiveBox (
								#{inParam01,mode=IN,jdbcType=INTEGER}
								,#{inParam02,mode=IN,jdbcType=INTEGER}
								,#{inParam03,mode=IN,jdbcType=INTEGER}
								,null
								,null
								,null
								,null
								,null
								,#{outParam01,mode=OUT,jdbcType=INTEGER}
								,#{outStrParam01,mode=OUT,jdbcType=VARCHAR})
	}
	</select>
</mapper>